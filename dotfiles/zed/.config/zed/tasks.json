[
  {
    "label": "json minify",
    "command": "json-minify $ZED_FILE > $TMPDIR/$ZED_FILENAME && mv $TMPDIR/$ZED_FILENAME $ZED_FILE",
    "use_new_terminal": false,
    "reveal": "never"
  },
  {
    "label": "list extensions",
    "command": "ls ~/Library/Application\\ Support/Zed/extensions/installed",
    "use_new_terminal": false,
    "reveal": "always"
  }
]
